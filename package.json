{
  "name": "cui-applet",
  "xuiConfig": {
    "name": "cui-applet",
    "icon": "xui/cui-applet.png",
    "met_description": "Hello World applet for extending the CloudBolt CUI",
    "met_label": "CUI Applet Hello World",
    "met_maximum_version_required": "",
    "met_minimum_version_required": "2023.1.1",
    "met_routing": {}
  },
  "private": true,
  "version": "0.0.0",
  "description": "Hello World applet for extending the CloudBolt CUI",
  "type": "module",
  "main": "dist/main.umd.js",
  "module": "dist/main.esm.js",
  "unpkg": "dist/main.min.js",
  "browser": {
    "./sfc": "src/Main.vue"
  },
  "scripts": {
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:es": "rollup --config --format es --file dist/main.esm.js --compact",
    "build:umd": "rollup --config --format umd --file dist/main.umd.js",
    "build:unpkg": "rollup --config --format iife --file dist/main.min.js --compact",
    "check:format": "prettier --check .",
    "check:lint": "eslint --ext .js,.vue --ignore-path .gitignore src",
    "co:login": "aws sso login && aws codeartifact login --tool npm --repository cloudbolt-npm --domain cloudbolt --domain-owner 499620025628 --namespace @cloudbolt",
    "format": "prettier . --write",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "makeXui": "xui",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@cloudbolt/cb-api-helper": "^0.5.0",
    "@cloudbolt/cb-xui-packager": "^1.0.0",
    "@rollup/plugin-buble": "^1.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "camelcase": "^7.0.1",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^9.4.0",
    "express": "^4.18.2",
    "prettier": "^2.7.1",
    "rollup": "^3.9.1",
    "rollup-plugin-vue": "^5.0.0",
    "vue": "^2.7.10",
    "vuetify": "^2.6.12"
  }
}
